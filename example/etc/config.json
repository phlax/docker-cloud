{
    "hostname": "sometesthost.foo.com",
    "modules": ["nfs", "nfsd"],
    "grub": {
	"timeout": "0"
    },
    "swap": {
	"size": "1G",
	"file": "/var/swap/swap.img"
    },
    "user": {
	"name": "apps"
    },
    "sysctl": {
	"vm.swappiness": "1",
	"vm.max_map_count": "262144"
    },
    "ssh": {
	"ports": ["222", "333", "444"]
    }
}
